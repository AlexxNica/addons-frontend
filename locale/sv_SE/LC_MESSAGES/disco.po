#~ msgstr "Installerad och tillagd till verktygsfältet"

#~ msgid "Click here to access %(name)s any time."
#~ msgstr "Klicka här för att komma åt %(name)s när som helst."

#~ msgid "Ok"
#~ msgstr "Ok"

#~ msgid "Preview %(name)s"
#~ msgstr "Förhandsgranska %(name)s"
