#~ msgstr "Установлено и добавлено на панель инструментов"

#~ msgid "Click here to access %(name)s any time."
#~ msgstr "Щёлкните здесь для доступа к %(name)s в любое время."

#~ msgid "Ok"
#~ msgstr "Ок"

#~ msgid "Preview %(name)s"
#~ msgstr "Предпросмотр %(name)s"
